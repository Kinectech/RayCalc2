{
  "appId": "medical.dosimetry.raycalc2",
  "appName": "RayCalc2",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "cordova": {
    "preferences": {
      "ScrollEnabled": "true",
      "KeyboardResize": "true",
      "KeyboardResizeMode": "native",
      "android-minSdkVersion": "19",
      "BackupWebStorage": "none",
      "resizeOnFullscreen": "true",
      "SplashMaintainAspectRatio": "true",
      "FadeSplashScreenDuration": "300",
      "SplashShowOnlyFirstTime": "false",
      "SplashScreen": "screen",
      "SplashScreenDelay": "3000"
    }
  },
  "plugins": {
    "Keyboard": {
      "resize": "ionic",
      "style": "dark",
      "setAccessoryBarVisible": "true"
    }
  },
  "server": {
    "url": "http://192.168.1.239:8100"
  }
}
